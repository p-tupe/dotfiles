<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
   "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.pritesh.nextcloud-cron</string>
    <key>ServiceDescription</key>
    <string>Run daily background cron jobs for nextcloud</string>
    <key>Program</key>
    <string>/Users/pritesh/.local/bin/nextcloud-cron</string>
    <key>RunAtLoad</key>
    <true />
    <key>StartOnMount</key>
    <true />
    <key>StandardErrorPath</key>
    <string>/tmp/com.pritesh.nextcloud-cron.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/com.pritesh.nextcloud-cron.out</string>
  </dict>
</plist>
