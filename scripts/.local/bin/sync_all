~/.local/bin/sync/servers

echo "Sync sandisk..."
~/.local/bin/sync/sandisk

echo "Sync SSD..."
~/.local/bin/sync/SSD

echo "Sync Pixel..."
~/.local/bin/sync/pixel

echo "Sync Mega..."
~/.local/bin/sync/mega

echo "Sync Nextcloud..."
~/.local/bin/sync/nextcloud
